    "devDependencies": {
        "babel-core": "^6.2.1",
        "babel-loader": "^6.2.0",
        "babel-preset-es2015": "^6.1.18",
        "babel-preset-react": "^6.1.18",
        "babel-preset-stage-1": "^6.1.18",
        "cross-env": "3.2.4",
        "css-loader": "^0.28.10",
